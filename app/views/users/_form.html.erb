<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :birth_year %>
    <%= f.input :gender %>
    <%= f.input :marital_status %>
    <%= f.input :children %>
    <%= f.input :party_affliation %>
    <%= f.input :email %>
    <%= f.input :telephone %>
  </div>

  <div class="form-actions">
    <%= f.button :submit,  "Click Me!" %>
  </div>
<% end %>
